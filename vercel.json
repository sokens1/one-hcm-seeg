{
  "rewrites": [
    {
      "source": "/((?!.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/favicon.ico",
      "destination": "/favicon.svg",
      "permanent": true
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/maintenance",
      "status": 200
    },
    {
      "src": "/maintenance",
      "dest": "/maintenance",
      "status": 200
    }
  ]
}