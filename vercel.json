{
  "rewrites": [
    {
      "source": "/((?!.*\\..*).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/favicon.ico",
      "destination": "/favicon.svg",
      "permanent": true
    },
    {
      "source": "/((?!maintenance).*)",
      "destination": "/maintenance",
      "permanent": false
    }
  ]
}